for float angle 0 ; angle < 360 ; set angle (angle+0.1)
    makeList grid "string" 20 "                    "
    cos angle into x
    sin angle into y
    int yi (y*10+20)
    int xi (x*10+20)
    setch grid[yi] xi "X"
    for int i 0 ; i < 20 ; incr i
        println grid[i]
    endw
    system "cls"
endw