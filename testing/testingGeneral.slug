# testing mutate

int toBeModified 60

func modifyLocally {int toThis}
    eval toBeModified (toBeModified/toThis)
    printf "LOCAL 1: " toBeModified
end

func modifyGlobally {int toThis}
    eval toBeModified (toBeModified/toThis)
    printf "LOCAL 2: " toBeModified
    mutate toBeModified
end

modifyLocally 2

printf "GLOBAL 1: " toBeModified

modifyGlobally 3

printf "GLOBAL 2: " toBeModified